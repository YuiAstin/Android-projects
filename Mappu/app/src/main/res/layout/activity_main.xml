<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="204dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:text="Đại học Khoa học Tự nhiên Tp. HCM" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="204dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView1"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:text="Đại Học Sài Gòn" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="204dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:text="Trường Đại học Sư Phạm TP. HCM" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="204dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView3"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:text="Chợ Hoa Hồ Thị Kỷ" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="204dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView4"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:text="Nhà Sách Nguyễn Văn Cừ - An Dương Vương" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="204dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView5"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:text="Chamichi Vietnam" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="204dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView6"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:text="Hotel Equatorial Ho Chi Minh City" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="204dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:text="Nhà khách Chính phủ" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="204dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView8"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:text="CLB Bơi lội Lam Sơn" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="204dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView9"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:text="Trường trung học Thực Hành Sài Gòn" />
</RelativeLayout>